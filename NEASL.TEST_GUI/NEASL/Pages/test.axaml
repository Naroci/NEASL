<pages:NEASL_Page xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:pages="clr-namespace:NEASL.PAGES"
                    xmlns:controls="clr-namespace:NEASL.CONTROLS"
                    mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="750"
                    >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="56" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Border  BoxShadow="2 2 10 0 DarkGray" Margin="-1,-1,-1,0" Height="">
            <Label Margin="10,0,0,0" Content="New Page!" FontSize="18" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        </Border>
        <StackPanel Grid.Row="1">
            <controls:NEASL_Button  Script="NEASL/Scripts/BUTTON2.neasl" />
            <controls:NEASL_Button Text="1123" Script="NEASL/Scripts/BUTTON.neasl" />
            <controls:NEASL_TextInput Script="NEASL/Scripts/TEXTINPUT.neasl" />
        </StackPanel>
    </Grid>
</pages:NEASL_Page>