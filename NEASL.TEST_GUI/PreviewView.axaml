<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:testGui="clr-namespace:NEASL.TEST_GUI"
             mc:Ignorable="d" d:DesignHeight="840" d:DesignWidth="386"
             Height="840" Width="386"
             ClipToBounds="True"
             x:Class="NEASL.TEST_GUI.PreviewView">
    <Border x:Name="CurrentPageDisplay" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="White" ClipToBounds="True" BorderThickness="2" BorderBrush="Gray" CornerRadius="25">
        <testGui:NEASL_Page />
    </Border>
</UserControl>
